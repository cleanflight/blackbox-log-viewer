    "del": "^3.0.0",
    "gulp": "~4.0.0",
    "gulp-concat": "~2.6.1",
    "gulp-debian": "~0.1.8",
    "gulp-install": "^1.1.0",
    "gulp-rename": "~1.2.2",
    "gulp-zip": "^4.1.0",
    "inflection": "1.12.0",
    "jquery-ui-npm": "1.12.0",
    "makensis": "^0.9.0",
    "nw-builder": "^3.4.1",
    "rpm-builder": "^0.6.1",
    "os": "^0.1.1",
    "platform-dependent-modules": "0.0.14",
